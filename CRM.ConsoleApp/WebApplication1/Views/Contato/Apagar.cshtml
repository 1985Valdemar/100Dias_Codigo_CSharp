@{
    ViewData["Title"] = "Apagar Contato";
}

<div class="text-center">

    <h1 class="display-4">Deletar Contato</h1>
    <h3>Realmente Deseja Apagar o Contato</h3>
</div>
<a class="btn btn-secondary" role="button" asp-controller="Contato" asp-action="Index">Cancelar Exclusão</a>
<a class="btn btn-secondary" role="button" asp-controller="Contato" asp-action="ApagarContato" >Confirmar Exclusão</a>
<a class="btn btn-secondary" role="button" asp-controller="Contato" asp-action="Index">Voltar</a>
